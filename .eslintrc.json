{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        // "eslint:recommended",
        // "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        // "react"
    ],
    "rules": {
        "semi": ["warn", "always"],
        "indent": "off",
        "no-unused-vars": "warn",
        "react/jsx-filename-extension": "warn"
    }
}
